package org.lambot3478.autonomous_step;

public abstract class AutonomousStep {
	// Parámetro límite (en caso de ser cuantitativo)
	protected double limit;
	// Método para inicializar el paso
	public abstract void start();
	// Función iterativa del paso
	public abstract void run();
	// Función que indica al autónomo si se terminó el paso
	public abstract boolean isFinished();
}
